<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cee.restaurantmanager">

    <application
        android:name=".FirebaseApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BookingManager" />
        <activity android:name=".OrderingManager" />
        <activity android:name=".AddBooking" />
        <activity android:name=".ViewBooking" />
        <activity android:name=".ViewByDate" />
        <activity android:name=".DeleteBooking" />
        <activity android:name=".PlaceOrder" />
        <activity android:name=".ViewOrder" />
        <activity android:name=".ViewByTable" />
        <activity android:name=".DeleteOrder"></activity>
    </application>

</manifest>